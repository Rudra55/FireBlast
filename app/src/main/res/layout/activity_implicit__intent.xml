<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.Implicit_Intent">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="facebook"
        android:id="@+id/fb"
        android:layout_centerHorizontal="true"
        android:background="#FFEB3B"
        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="enter url text"
        android:id="@+id/url1"
        android:layout_below="@+id/fb"
        android:layout_centerHorizontal="true"
        />
    <Button
        android:id="@+id/click"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="click to open url"
        android:layout_below="@+id/url1"
        android:layout_centerHorizontal="true"

        />
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint="enter no for call"
        android:id="@+id/call"
        android:layout_below="@+id/click"
        android:layout_centerHorizontal="true"
        />
    <Button
        android:id="@+id/click1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="call"
        android:layout_below="@+id/call"
        android:layout_centerHorizontal="true"

        />
    <Button
        android:id="@+id/btn3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dail"


        />


</RelativeLayout>